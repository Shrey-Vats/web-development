
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int     @unique @default(autoincrement())
  username   String  @unique
  email      String  @unique 
  password   String  
  provider   String  @default("local")
  googleId   String? @unique
}